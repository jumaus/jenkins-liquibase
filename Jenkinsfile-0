pipeline {
	agent {
		docker {
			label 'docker'
			image 'liquibase/liquibase:4.4.2'
		}
	}
	environment {
		DB_CREDS=credentials('ora-db-hr')
	}
	stages {
		stage('Test') {
			steps {
				sh 'liquibase generate-changelog --changelog-file=changeLogUsrHr.sql --url="jdbc:oracle:thin:@10.0.10.13/orcl" --changeLogFile=my_app-wrapper.xml --username=$DB_CREDS_USR --password=$DB_CREDS_PSW'
			}
		}
	}
}